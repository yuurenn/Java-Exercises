package enshu2_2022;

public class kuku {
	public static void main(String[] args) {
        int z;
        System.out.print("　　　");
        for (int i = 1; i < 10; i++) {
            System.out.print(i + "　");
        }
        System.out.println();
        for (int i = 1; i < 10; i++) {
            System.out.print("----");
        }
        System.out.println();
        for (int i = 1; i < 10; i++) {
            System.out.print(i + ("　｜"));
            for (int j = 1; j < 10; j++) {
                z = i * j;
                if (z < 10) {
                    System.out.print((" "));
                }
                System.out.print(z + (" "));
            }
            System.out.println();
        }
    }
}
