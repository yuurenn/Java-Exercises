package enshu2_2022;

public class kuku2 {
    public static void main(String[] args) { // main メソッド
        int y;
        y = 1;
        while (y < 10) {
            sub(y); // sub メソッドの呼び出し（元のプログラムの 6～12 行に相当）
            System.out.println();
            y++;
        }
    }

    public static void sub(int y) { // sub メソッド
        int x;
        for (x = 1; x < 10; x++) {
            if (x * y < 10) {
                System.out.print(" " + x * y + " ");
            } else {
                System.out.print(x * y + " ");
            }
        }
    }
}